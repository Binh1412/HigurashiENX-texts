[
	{
		"type": "SAVEINFO",
		"category": 1,
		"JPN": "可愛い子だったね",
		"ENG": "可愛い子だったね"
	},
	{
		"type": "MSGSET",
		"MessageID": 17119,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS20/01/VTT_keiichi3001.",
		"TextJPN": "「そういや魅音。@k@vS20/01/VTT_keiichi3002.さっきの夏美ちゃんと親しそうだったけど、仲良かったのか？」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17120,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS20/03/VTT_mion3001.",
		"TextJPN": "「ん？　まぁね。@k@vS20/03/VTT_mion3002.結構人見知りする子だったから、こっちから誘わないとなかなか輪に入ってこなくって。@k@vS20/03/VTT_mion3003.それで構っているうちに、自然にね」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17121,
		"NamesJPN": "レナ",
		"PreTextTags": "@vS20/02/VTT_rena3001.",
		"TextJPN": "「かぁいい女の子だったよねぇ？@k@vS20/02/VTT_rena3002.　並んでると魅ぃちゃん、夏美ちゃんのお姉さんみたいだったよ」",
		"NamesENG": "Rena",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17122,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS20/03/VTT_mion3004.",
		"TextJPN": "「あっはっはっはっ！@k@vS20/03/VTT_mion3005.　…ちなみに言っとくけどレナ、夏美ちゃんは年上だよ？@k@vS20/03/VTT_mion3006.　実は私よりも」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17123,
		"NamesJPN": "レナ",
		"PreTextTags": "@vS20/02/VTT_rena3003.",
		"TextJPN": "「そうなの？@k@vS20/02/VTT_rena3004.　でも子猫さんみたいにかぁいかったよ～、はぅ♪」",
		"NamesENG": "Rena",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17124,
		"TextJPN": "…早めにこの場を離れて正解だったな、夏美ちゃん。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17125,
		"TextJPN": "もう少し話し込んでいたら、レナに捕食されてお持ち帰りされているところだ。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17126,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS20/03/VTT_mion3007.",
		"TextJPN": "「でも、女の子って変わるもんだね～。@k@vS20/03/VTT_mion3008.知り合った頃は言っちゃぁ悪いけど、陰気なイメージだったからさ。@k@vS20/03/VTT_mion3009.気難しい上につかみどころがなくって、…なんか付き合いづらい雰囲気だったんだ」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17127,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS20/01/VTT_keiichi3003.",
		"TextJPN": "「へぇ…？@k@vS20/01/VTT_keiichi3004.　なんか、想像がつかないよな」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17128,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS20/03/VTT_mion3010.",
		"TextJPN": "「でも、中学を卒業してからは、すっごく明るくなってね。今みたいに。@k@vS20/03/VTT_mion3011.それを見て、ああ、成長するってこういうことなんだなぁ～、なんて、おじさんは思ったもんだよ」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17129,
		"TextJPN": "魅音はそう言って、しみじみと懐かしそうに遠い目をする。…ていうか魅音、そういうしぐさは『お姉さん』と言うよりも、むしろ『おばさん』と呼ぶ以上に年寄りじみて見えるんだが。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17130,
		"NamesJPN": "レナ",
		"PreTextTags": "@vS20/02/VTT_rena3005.",
		"TextJPN": "「ということは、…魅ぃちゃんもいずれ変わっちゃうってことだよね？@k@vS20/02/VTT_rena3006.　魅ぃちゃんが成長したらどうなるのかな？　かな？」",
		"NamesENG": "Rena",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17131,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS20/03/VTT_mion3012.",
		"TextJPN": "「あっはっはっはっ、決まってるじゃ～ん！@k@vS20/03/VTT_mion3013.　気品と知性にあふれた、男子学生からの羨望を一身に集めるアイドルか、マドンナって感じになっちゃうんだろうね～？」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17132,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS20/01/VTT_keiichi3005.",
		"TextJPN": "「……下品と痴性？」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17133,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS20/03/VTT_mion3014.",
		"TextJPN": "「圭ちゃん、…私になんか恨みでも？」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17134,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS20/01/VTT_keiichi3006.",
		"TextJPN": "「い、いや、そんなことは…」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17135,
		"NamesJPN": "レナ",
		"PreTextTags": "@vS20/02/VTT_rena3007.",
		"TextJPN": "「はぅ…、でもそんな魅ぃちゃん、レナも想像できないよ～」",
		"NamesENG": "Rena",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17136,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS20/03/VTT_mion3015.",
		"TextJPN": "「ちょっとぉ、レナまでそんなこと言うのぉ～？！」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17137,
		"TextJPN": "確かに、…魅音はいつまでもこんな感じで、男勝りにぐいぐいとみんなを引っ張っていく姿が一番似合っているような気がする。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17138,
		"TextJPN": "清楚可憐な、お嬢様のように可愛らしい魅音も見てみたいけど、…俺としては仲間として思いっきり笑ったり、盛大にふざけあえるこの関係が結構、気に入っていた。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17139,
		"TextJPN": "……あれ？",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17140,
		"TextJPN": "そういえば魅音のことばっかり話しているけど、レナこそどうなんだろう？　成長したら、やっぱり今とは変わってしまうんだろうか。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17141,
		"NamesJPN": "レナ",
		"PreTextTags": "@vS20/02/VTT_rena3008.",
		"TextJPN": "「え、…レナ？@k@vS20/02/VTT_rena3009.　レナはレナのまんまだよ。@k@vS20/02/VTT_rena3010.今もこれからも、ずーっと同じ。@k@vS20/02/VTT_rena3011.はぅ♪」",
		"NamesENG": "Rena",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17142,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS20/01/VTT_keiichi3007.",
		"TextJPN": "「…ということは、今と同じかぁいいもの好きで、見境なくお持ち帰りする女性、おばさん、おばあさんって感じになるのか」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17143,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS20/03/VTT_mion3016.",
		"TextJPN": "「なんか嫌だね、それ。@k@vS20/03/VTT_mion3017.…一歩間違えたら、窃盗犯か誘拐犯だよ」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17144,
		"NamesJPN": "レナ",
		"PreTextTags": "@vS20/02/VTT_rena3012.",
		"TextJPN": "「そ、そんなことないよ～？@k@vS20/02/VTT_rena3013.　大人になったら、ちゃんと最低限の常識を守って行動するようになるから！@k@vS20/02/VTT_rena3014.　…きっと」",
		"NamesENG": "Rena",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17145,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS20/01/VTT_keiichi3008.",
		"TextJPN": "「いや、それはもちろんそうかもしれないが。@k@vS20/01/VTT_keiichi3009.…むしろレナ。@k@vS20/01/VTT_keiichi3010.今の発言は自分の行動が『非常識』だと、まるで肯定しているようにも聞こえるぞ？」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17146,
		"NamesJPN": "レナ",
		"PreTextTags": "@vS20/02/VTT_rena3015.",
		"TextJPN": "「はぅっっ？！」",
		"NamesENG": "Rena",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17147,
		"TextJPN": "…墓穴を掘ったな、レナ。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17148,
		"TextJPN": "それにしても、…レナと魅音の成長した姿か。実際にそれを見たとき、俺はどんなリアクションをするんだろうか？",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 17149,
		"TextJPN": "それはそれで、今から楽しみだ。",
		"TextENG": ""
	}
]