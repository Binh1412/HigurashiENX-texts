[
	{
		"type": "SAVEINFO",
		"category": 1,
		"JPN": "#5 珍事の予兆",
		"ENG": "#5 珍事の予兆"
	},
	{
		"type": "MSGSET",
		"MessageID": 73330,
		"TextJPN": "楽しいはずの自習の時間だったが、どうも今日は盛り上がりに欠けた。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73331,
		"TextJPN": "レナの様子が朝から変だったからだ。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73332,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300001.",
		"TextJPN": "「さっきから調子悪そうだね。@k@vS07/03/330300002.…今日は体調悪いの？」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73333,
		"NamesJPN": "レナ",
		"PreTextTags": "@vS07/02/330200082.",
		"TextJPN": "「…うぅん、体調悪くないよぅ。@k@vS07/02/330200083.……はぅ…」",
		"NamesENG": "Rena",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73334,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400069.",
		"TextJPN": "「また溜め息をついていますわね。@k@vS07/04/330400070.溜め息をひとつつく度に幸せが逃げてっちゃいますのよー？」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73335,
		"NamesJPN": "レナ",
		"PreTextTags": "@vS07/02/330200084.",
		"TextJPN": "「…うん。@k@vS07/02/330200085.……幸せ…。@k@vS07/02/330200086.……はぅ…」",
		"NamesENG": "Rena",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73336,
		"TextJPN": "登校した時はこれほどではなかったのだが…。やっぱり風邪なのだろうか。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73337,
		"TextJPN": "朝からどこかぼんやりとしていて微熱がありそうな感じだった。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73338,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300003.",
		"TextJPN": "「保健室で風邪薬でももらう？@k@vS07/03/330300004.　最近、昼夜の温度差が激しいからね」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73339,
		"NamesJPN": "レナ",
		"PreTextTags": "@vS07/02/330200087.",
		"TextJPN": "「……うぅん、いい。@k@vS07/02/330200088.…今日はその、…レナは放っておいてくれないかな。@k@vS07/02/330200089.…かな」",
		"NamesENG": "Rena",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73340,
		"TextJPN": "レナはぽやんとした口調でそれだけを言うと、俺たちの輪を抜け、自分の席へ戻ってしまった。そして頬杖を突きながら、窓の外を見て再び溜め息を漏らす。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73341,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300005.",
		"TextJPN": "「ふふ～～ん？@k@vS07/03/330300006.　おじさんの見たところ、あれは恋だねぇ！」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73342,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400071.",
		"TextJPN": "「ここ、恋ですってぇええぇ？！@k@vS07/04/330400072.　あわわわわ、そそ、それはその、をほほ、おめでたいことでございますわねぇ！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73343,
		"NamesJPN": "詩音",
		"PreTextTags": "@vS07/06/330600006.",
		"TextJPN": "「へぇぇ～？@k@vS07/06/330600007.　レナさんもなかなかやりますねぇ…ひゅーひゅー！」",
		"NamesENG": "Shion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73344,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300007.",
		"TextJPN": "「…というか詩音、あんた学校はどうしたのさ？@k@vS07/03/330300008.　いくら自習中でも、勝手に入ってきちゃ駄目じゃないの」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73345,
		"NamesJPN": "詩音",
		"PreTextTags": "@vS07/06/330600008.",
		"TextJPN": "「ままま、細かいことは気にしない。@k@vS07/06/330600009.それはともかく、間違いありませんね！@k@vS07/06/330600010.　心ここに在らずで、一日中想い人のことでいっぱいって感じですよ。@k@vS07/06/330600011.頬杖を突いて溜め息なんて、恋する乙女そのものじゃないですか～！」",
		"NamesENG": "Shion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73346,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100080.",
		"TextJPN": "「そ、そうなのかぁ…？@k@vS07/01/330100081.　俺には、その、…昨日、飲み込んじまった妙なもんのせいで体調を崩してるようにしか見えないんだがなぁ…」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73347,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400073.",
		"TextJPN": "「…妙なものって、何ですの？」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73348,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100082.",
		"TextJPN": "「ん？@k@vS07/01/330100083.　あぁ。昨日さ、宝探しに行った先でさ。@k@vS07/01/330100084.…何なんだろうなぁ、何か降って来たみたいなんだよ。@k@vS07/01/330100085.そいつがスポンと口ん中に飛びこんじまって、それを飲み込んじゃったらしいんだよ」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73349,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300009.",
		"TextJPN": "「天から降って来たぁ？@k@vS07/03/330300010.　あははははは！@k@vS07/03/330300011.　ならきっと、それはキューピッドの恋の矢だねぇ」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73350,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500047.",
		"TextJPN": "「……天から、降って来た？」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73351,
		"TextJPN": "俺たちがレナの話に夢中になっている隙に、沙都子の席にこっそりとカエルの玩具を仕掛けていた梨花ちゃんが、猫耳をぴくんと反応させて振り向いた。…人間の耳って側面だよな？　今のは錯覚か。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73352,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500048.",
		"TextJPN": "「……圭一。@k@vS07/05/330500049.レナがどうしたのですか？」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73353,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100086.",
		"TextJPN": "「うん、いやな？@k@vS07/01/330100087.　昨日、ダム現場に新しいゴミ山が出来たとかで、宝探しに出掛けたんだよ」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73354,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100088.",
		"TextJPN": "「んで、いつものように馬鹿騒ぎして、大口開けて空を見上げて笑ってたら、そこに何かが落ちてきて、綺麗に口の中に飛び込んで、思わず飲み込んじまったってんだよ」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73355,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500050.",
		"TextJPN": "「……天から落ちてきたものが、口の中に？@k@vS07/05/330500051.　飲み込んだ？」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73356,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400074.",
		"TextJPN": "「…お聞きになりましたか、梨花？」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73357,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400075.",
		"TextJPN": "「って、あなた私の席に何をしようとしているのでございますかっ？！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73358,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500052.",
		"TextJPN": "「にぱ～、って、それは後回しなのです。@k@vS07/05/330500053.…え？　み、みぃっ！@k@vS07/05/330500054.　大変なのです！！」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73359,
		"TextJPN": "すっとんきょうな叫びが聞こえて、俺は振り返る。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73360,
		"TextJPN": "すると梨花ちゃんが、うっとりとした表情を浮かべながら溜め息をこぼすレナにおそるおそる、近づいていくのが見えた。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73361,
		"TextJPN": "その手には、なぜか派手なつくりの方位磁石。そして、しばらく様子をうかがった後……彼女には珍しく、驚きに目を見開いた表情を浮かべる。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73362,
		"TextJPN": "それからだっ、と沙都子に駆け寄り、教室の隅へと離れていった。……どうしたんだろう？",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73363,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500055.",
		"TextJPN": "「み、みみみみ…みぃぃッッ？！」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73364,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400076.",
		"TextJPN": "「ひゃぁああっ？！@k@vS07/04/330400077.　なな、何て声をお出しになるのですかー！@k@vS07/04/330400078.　ってことは、まさか？！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73365,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500056.",
		"TextJPN": "「レレ、レナの中に、勾玉の気配があるのです！@k@vS07/05/330500057.　勾玉の力がレナに宿ってしまったのです～！」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73366,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400079.",
		"TextJPN": "「あら、それなら話は早いですわね。@k@vS07/04/330400080.通りすがりにローブローでも叩き込んでみますか？@k@vS07/04/330400081.　…反撃で殺されますわね。@k@vS07/04/330400082.なら下剤でも飲ませてみます？@k@vS07/04/330400083.　…梨花がやってくださいませ！！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73367,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500058.",
		"TextJPN": "「そんな簡単な問題じゃないのです、みぃ！」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73368,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500059.",
		"TextJPN": "「昨日読んだ古文書によると、勾玉が体内に飲み込まれると溶けてしまうとありましたのです。@k@vS07/05/330500060.勾玉はレナに食べられたことで、形の意味を失い、その力をレナに宿らせてしまったのですよ～！」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73369,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400084.",
		"TextJPN": "「つまり、何ですの？@k@vS07/04/330400085.　あの勾玉は服用可能で、飲んだら消化吸収できるってことでございますか？」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73370,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400086.",
		"TextJPN": "「どど、どうしたらよろしいのですかっ？@k@vS07/04/330400087.　梨花のご先祖さまが長年封印していたアイテムでございましょう、何とかなりませんの？！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73371,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500061.",
		"TextJPN": "「みぃ、もちろん何とかはできますのです。@k@vS07/05/330500062.紅白の勾玉を揃え、オヤシロさまの神域、つまり古手神社の境内で解呪のおまじないをすれば、ちゃんと解けますのです」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73372,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400088.",
		"TextJPN": "「なら簡単ですわ！@k@vS07/04/330400089.　帰りにレナさんを神社に連行して一丁上がりですわ！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73373,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500063.",
		"TextJPN": "「…ところが、レナが飲み込んだのは赤の勾玉だけなのです。@k@vS07/05/330500064.白の勾玉が一緒になければ解呪はできないのです。@k@vS07/05/330500065.このままだとレナは、白の勾玉を拾った人を無差別に好きになってしまうのです」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73374,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500066.",
		"TextJPN": "「レナに赤の勾玉の効果が表れているということは、誰かが白の勾玉を拾ったということなのです。@k@vS07/05/330500067.だからレナは、今その人のことで頭がいっぱいになってるに違いないのですよ」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73375,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500068.",
		"TextJPN": "「みぃ、もしレナがこのまま暴走してハッピーエンドを迎えてしまったら、レナの一生は台無しになってしまいますのです、…みぃ！」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73376,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400090.",
		"TextJPN": "「り、り、梨花ぁああぁああぁーッ！！！@k@vS07/04/330400091.　あなたのご先祖さまが封印していたマジックアイテムなんですから、梨花が責任をお取りなさいませっ！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73377,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400092.",
		"TextJPN": "「何だって、こんな危険なアイテムが古手神社の秘宝なんですの～！！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73378,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500069.",
		"TextJPN": "「使い方さえ間違わなければ縁結びの素敵な効果なのです。@k@vS07/05/330500070.それは間違いないことなのです、みぃ！」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73379,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400093.",
		"TextJPN": "「それを巡って大騒動が昔に起こって、大変なことになったんでございましょう？！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73380,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400094.",
		"TextJPN": "「あぁ、今やっと理解できましたわ、何で梨花のご先祖さまが封印したかよくわかりましたわ！@k@vS07/04/330400095.　オヤシロさまって、何て傍迷惑な神さまなんでございますかっ！！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73381,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500071.",
		"TextJPN": "「…みぃ、それを言ったら大昔の村の人たちも、もう少し神社に寄付をしてくれていたらよかったのです」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73382,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500072.",
		"TextJPN": "「ちゃんとまとまった寄付があれば、材料と職人さんにもお金をかけて、もっとしっかりした香炉を作って封じ込めることができたかもしれないのですよ～！」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73383,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400096.",
		"TextJPN": "「最初から税金に頼るなんて、能力のない政治家が使う言い訳ですわっ！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73384,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400097.",
		"TextJPN": "「そもそもちゃんとした社会保障が必要なら、もっと税金を納めろですって？！@k@vS07/04/330400098.　だったら一日数台しか通らない道路に何億も使わないでほしいのですわ！！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73385,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400099.",
		"TextJPN": "「しかも、予算を余らせたら来年から割り当て額が減るからって、年末は帳尻合わせの工事ばっかり！！@k@vS07/04/330400100.　興宮のおもちゃ屋の前の道路なんて、毎年掘り返しているじゃありませんかっ？！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73386,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400101.",
		"TextJPN": "「そのくせ一般市民の医療負担は増える一方なんて、どう考えてもおかしすぎるのでございますわっ！！@k@vS07/04/330400102.　あぁ、こんなこと梨花に申し上げても仕方のないことでございますけど、ほんと納得のいかない話なのでしてよー？！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73387,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500073.",
		"TextJPN": "「みぃ、参政権のないボクたちがここで何を言っても仕方がないのです！@k@vS07/05/330500074.　不満があれば２０歳になってからちゃんと選挙の投票に行く！@k@vS07/05/330500075.　それが民主主義のルールなのです！」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73388,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400103.",
		"TextJPN": "「こんなところで選挙のキャンペーン活動をしていても仕方ありませんでしょう？！@k@vS07/04/330400104.　梨花ぁ、こうなっては梨花が過去のご先祖さまの責任を取って、あなたが罰を受けるべきでございますわ！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73389,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400105.",
		"TextJPN": "「私がこの前苦しんだ、詩音さんお手製の『こちょこちょマシーン・フルコース』はいかがかしらぁっ？！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73390,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500076.",
		"TextJPN": "「み、みみ、みいいぃぃいぃッッ！！@k@vS07/05/330500077.　さ、ささ、沙都子は鬼なのです！@k@vS07/05/330500078.　鬼、悪魔、人でなしなのですっ！！@k@vS07/05/330500079.　あ、あれは人が持つべき道具ではないのですよっ？！」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73391,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400106.",
		"TextJPN": "「をっほっほっ、をーっほっほっほっ！！@k@vS07/04/330400107.　お可哀想ですけど、覚悟なさいませ！！@k@vS07/04/330400108.　私が前に詩音さんから味わわされている横で盛大に笑った分、苦しみを存分にお食らいあそばせー、でございますわぁ！！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73392,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400109.",
		"TextJPN": "「……って、ふにゃあぁっ？」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73393,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500080.",
		"TextJPN": "「みぃっ…？！」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73394,
		"TextJPN": "突然、沙都子と梨花の背中がつまみ上げられる。…二人は子猫？　ふにゃん。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73395,
		"TextJPN": "つまみ上げたのは俺と詩音だった。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73396,
		"NamesJPN": "詩音",
		"PreTextTags": "@vS07/06/330600012.",
		"TextJPN": "「…沙都子と梨花ちゃまが何をじたばたしているのかわかりませんけど。@k@vS07/06/330600013.何かご存じなんですか？@k@vS07/06/330600014.　レナさんのあれ！」",
		"NamesENG": "Shion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73397,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400110.",
		"TextJPN": "「…梨花ぁ？」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73398,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500081.",
		"TextJPN": "「…みぃ？@k@vS07/05/330500082.　み～♪」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73399,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100089.",
		"TextJPN": "「…沙都子はあとでシメ上げるとして、梨花ちゃん語の翻訳にはみ～リンガルとか必要だよなぁ。@k@vS07/01/330100090.これはつまりどっちの意味なんだ？」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73400,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300012.",
		"TextJPN": "「こーのすっとぼけ方はクロだねぇ。@k@vS07/03/330300013.あんたたち、レナに何かしたの？！」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73401,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400111.",
		"TextJPN": "「……まぁ、もうすっとぼけても仕方ありませんわね」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73402,
		"TextJPN": "とにかくレナをこのままにはしておけない。二人は大人しく白状して、協力を仰ぐことにした…。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73403,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100091.",
		"TextJPN": "「古手神社の至宝、フワラズの勾玉ぁ？！」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73404,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500083.",
		"TextJPN": "「…はいなのです。@k@vS07/05/330500084.不和知らずに通じると言われる、縁結びのありがた～ぃ（迷惑な）宝物なのですよ」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73405,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300014.",
		"TextJPN": "「巻物の挿絵を見る限りじゃ、…オットセイのキーホルダーにしか見えないねぇ？」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73406,
		"NamesJPN": "詩音",
		"PreTextTags": "@vS07/06/330600015.",
		"TextJPN": "「同感ですね。@k@vS07/06/330600016.口の中に飛び込まなくても、レナさんならお持ち帰りしそうな形状をしています。@k@vS07/06/330600017.でもこの宝物、なんだか可愛い形ですね～」",
		"NamesENG": "Shion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73407,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100092.",
		"TextJPN": "「オットセイは繁殖力旺盛だから、熱愛のシンボルだーってテレビで見た気がするぜ？」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73408,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300015.",
		"TextJPN": "「へー…！@k@vS07/03/330300016.　だとすると、オットセイという生き物の存在が、数百年、あるいは数千年以上も前に日本、それも古手神社に伝来していた証拠になるねぇ？！」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73409,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300017.",
		"TextJPN": "「なるほど、これはとんでもないオーパーツかもしれないよ」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73410,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100093.",
		"TextJPN": "「しかし、わからねぇなぁ！@k@vS07/01/330100094.　何でそんな宝物が空から降ってきてレナの口の中に飛び込むんだ？」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73411,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500085.",
		"TextJPN": "「…にぱ～☆」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73412,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100095.",
		"TextJPN": "「しかし、オヤシロさまの宝物が縁結びとは、ちょいと意外だなぁ？@k@vS07/01/330100096.　もうちょっと祟られたり呪われたりしそうなイメージがあるんだが」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73413,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300018.",
		"TextJPN": "「確かに、近年はオヤシロさまの祟りとか言われて、イメージがずいぶん物騒になっちゃったけど、本来のオヤシロさまは縁結びの神さまでもあるんだよね。@k@vS07/03/330300019.…仲良くしないと祟る、ってのがメインだけど」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73414,
		"NamesJPN": "詩音",
		"PreTextTags": "@vS07/06/330600018.",
		"TextJPN": "「そうですね。@k@vS07/06/330600019.昔の古手神社では、縁結びのお札とかを売っていたことを覚えていますよ。@k@vS07/06/330600020.私も買っておけばよかったなぁ…ちぃぃ」",
		"NamesENG": "Shion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73415,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100097.",
		"TextJPN": "「んで、……その勾玉の縁結びの効果は、単なる気休めじゃなくて、霊験新たかなホンモノってわけなんだな？！」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73416,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500086.",
		"TextJPN": "「……そうなのです。@k@vS07/05/330500087.だから、レナは今、フワラズの勾玉の力に取り憑かれてしまっているのです」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73417,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300020.",
		"TextJPN": "「えっと、赤と白でペアなんだっけ？@k@vS07/03/330300021.　赤を持つ人が、白を持つ人を好きになる、だっけ？@k@vS07/03/330300022.　相思相愛じゃなくて片道なんだねぇ」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73418,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500088.",
		"TextJPN": "「……はいなのです。@k@vS07/05/330500089.レナはその赤を飲み込んでしまいましたですから、白を誰かが拾えば、もれなくレナも付いてきちゃうわけなのです。@k@vS07/05/330500090.みー」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73419,
		"NamesJPN": "詩音",
		"PreTextTags": "@vS07/06/330600021.",
		"TextJPN": "「な、何だか珍妙な話になってきましたねぇ！」",
		"NamesENG": "Shion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73420,
		"NamesJPN": "詩音",
		"PreTextTags": "@vS07/06/330600022.",
		"TextJPN": "「…そしてレナさんを元に戻すには、白の勾玉も見つけなくてはならないというわけですか？」",
		"NamesENG": "Shion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73421,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100098.",
		"TextJPN": "「しかも、レナの様子から見るに、誰かがすでに白の勾玉は拾っちまったわけだろ？！@k@vS07/01/330100099.　このまま放っておくと、レナはどんどんその人のことが好きになっていっちまうわけか。@k@vS07/01/330100100.レナの本当の気持ちとは別に」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73422,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300023.",
		"TextJPN": "「ちょっとロマンチックな話ではあるけど、相手がどこの馬の骨かもわからないってのは、ちょっといただけないねぇ。@k@vS07/03/330300024.拾いさえすれば、誰にでも惚れちゃうわけでしょ？」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73423,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500091.",
		"TextJPN": "「……そうです。@k@vS07/05/330500092.拾った人が、子供だろうとお年寄りだろうと、男女お構いなしなのです。@k@vS07/05/330500093.ましてや相手が家族持ちだった日には色々と悲劇なのですよ、にぱ～☆」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73424,
		"NamesJPN": "詩音",
		"PreTextTags": "@vS07/06/330600023.",
		"TextJPN": "「それは面白そ、…あ、いやげふんげふん！　それは大変ですねぇ。@k@vS07/06/330600024.どうやら事態は、一刻を争うって感じですか？！」",
		"NamesENG": "Shion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73425,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500094.",
		"TextJPN": "「一刻を争いますのです。@k@vS07/05/330500095.しかも、食べても作用するので、もし通りかかった犬さんが食べてしまったら、……それはもう大変なことになってしまうのです」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73426,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100101.",
		"TextJPN": "「い、いいい、犬ぅっっ？！@k@vS07/01/330100102.　それって動物にまで有効なのかよ？！」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73427,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300025.",
		"TextJPN": "「さ、さすがに可愛いからって犬とカップルになるのは、おじさん、ちょっとパスだなぁ…」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73428,
		"NamesJPN": "詩音",
		"PreTextTags": "@vS07/06/330600025.",
		"TextJPN": "「…むしろＯＫの人がいたらお聞きしてみたいものです」",
		"NamesENG": "Shion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73429,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400112.",
		"TextJPN": "「どうしてですの？@k@vS07/04/330400113.　レナさんと犬が仲良くなるだけなら、一番被害は軽微でしてよ？？」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73430,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500096.",
		"TextJPN": "「…沙都子も恋愛をするようになれば、その意味がわかると思うのですよ。@k@vS07/05/330500097.なでなで☆」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73431,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100103.",
		"TextJPN": "「とにかく、人心を惑わす魔性の勾玉だってことは理解できたぜ。@k@vS07/01/330100104.手遅れにならん内にどうにかした方がいいな！」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73432,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300026.",
		"TextJPN": "「そうだね。@k@vS07/03/330300027.恋愛は自由意志が大事だよ。@k@vS07/03/330300028.魔法の力で強制されるなんて、よくないね！」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73433,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300029.",
		"TextJPN": "「…まぁ、ちょっぴりはロマンチックだと思うけどさ！」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73434,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500098.",
		"TextJPN": "「…とにかく、取り返しがつかなくなる前に何とかしましょうです。@k@vS07/05/330500099.じゃないとこれはこれで大惨事なのですよ」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73435,
		"NamesJPN": "詩音",
		"PreTextTags": "@vS07/06/330600026.",
		"TextJPN": "「問題は、白のオットセイを誰が拾ったかですね。@k@vS07/06/330600027.いくら雛見沢が田舎とはいえ、どこの誰が拾ったのかわからないんじゃ、手の打ちようがありませんし」",
		"NamesENG": "Shion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73436,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300030.",
		"TextJPN": "「うん？@k@vS07/03/330300031.　そりゃ簡単じゃない！@k@vS07/03/330300032.　だって、魔法の力で、レナは白の勾玉を持った人を好きになるんでしょ？@k@vS07/03/330300033.　なら、レナの頭の中はその人のことでいっぱいだよ！@k@vS07/03/330300034.　つまり、レナに直接聞けばいいわけさ！」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73437,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300035.",
		"TextJPN": "「レぇナぁ～～！！！@k@vS07/03/330300036.　おじさんに教えてよぅ！@k@vS07/03/330300037.　レナのハートを射止めちゃった意中の相手はだぁれぇえぇ？」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73438,
		"TextJPN": "……レナ乱舞って今日まで色々見てきたけど、コイツはこれまでの中で一番長かった。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73439,
		"NamesJPN": "レナ",
		"PreTextTags": "@vS07/02/330200090.",
		"TextJPN": "「…は、…はぅうぅぅ、そんなこと言えないよぅ…。@k@vS07/02/330200091.はぅ～！」",
		"NamesENG": "Rena",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73440,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100105.",
		"TextJPN": "「…い、いや、多分こうなると思ったよ。@k@vS07/01/330100106.しかしずいぶん威力が向上したな…。@k@vS07/01/330100107.ヒット数も大幅に増加してるぞ…」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73441,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500100.",
		"TextJPN": "「……次のパッチでは弱体化に違いないのですよ。@k@vS07/05/330500101.にぱ～☆」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73442,
		"NamesJPN": "詩音",
		"PreTextTags": "@vS07/06/330600028.",
		"TextJPN": "「大丈夫ですか、お姉。@k@vS07/06/330600029.女の子に好きな人のことを直接聞くなんて、相変わらずデリカシーがなさすぎですよ」",
		"NamesENG": "Shion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73443,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300038.",
		"TextJPN": "「……ごめんねぇ、おじさん、女の子じゃないもんでわかんなかったわぁ…」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73444,
		"TextJPN": "あああぁあぁ、もう厄介なことになったなぁぁ…。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73445,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400114.",
		"TextJPN": "「それにしても、…よりによってレナさんなのですか？@k@vS07/04/330400115.　恋心がそのまま攻撃力に転換されるようなお方なんですのよ？@k@vS07/04/330400116.　何だか話がこじれそうですわ…」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73446,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400117.",
		"TextJPN": "「梨花ぁ、元はといえば古手神社の管理が悪かったからなのですから、あなたも対策を何か考えなさいませ！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73447,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500102.",
		"TextJPN": "「…みぃ、こちょこちょマシーンの刑を無しにしてくれるのなら、もっと真剣に考えてみますのですよ…！」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73448,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400118.",
		"TextJPN": "「あらぁ、そうでございますかぁ？@k@vS07/04/330400119.　じゃあ軽くお試しに体験してみて、その恐怖のプレッシャーで気合を入れるってのはいかがかしらぁ！@k@vS07/04/330400120.　詩音さん詩音さん、ちょっとお話が――」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73449,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500103.",
		"TextJPN": "「か、考えますのです！！」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73450,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500105.",
		"TextJPN": "「みぃ、あれです！」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73451,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400121.",
		"TextJPN": "「…ずいぶん投げ遣りに思いついたようでございますけど、何なんですか？」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73452,
		"TextJPN": "梨花ちゃんが指差したのはレナの席だった。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73453,
		"TextJPN": "……レナはさすがに頬杖に飽きたのか、読書を始めていた。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73454,
		"TextJPN": "時には悪ふざけを抜け読書をすることもある。別に珍しい光景でもなかった。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73455,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400122.",
		"TextJPN": "「…でも何だか様子が違いますわね。@k@vS07/04/330400123.普段とは違う雰囲気の本を何冊も持ってきておりますわ」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73456,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500106.",
		"TextJPN": "「どうかしましたか、沙都子」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73457,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400125.",
		"TextJPN": "「…レナさんの様子をよぉく御覧なさいませ。@k@vS07/04/330400126.何かヒントになるかもしれませんわ」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73458,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300039.",
		"TextJPN": "「何読んでんだろ。@k@vS07/03/330300040.ここからじゃよくわかんないね…」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73459,
		"TextJPN": "さっきのレナの攻撃で懲りたのか、魅音は必要以上に距離を取って慎重そうだった。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73460,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100108.",
		"TextJPN": "「何だ？@k@vS07/01/330100109.　ありゃ多分、何かのハウツー本だな。@k@vS07/01/330100110.何を勉強しようとしてるんだ？」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73461,
		"NamesJPN": "詩音",
		"PreTextTags": "@vS07/06/330600030.",
		"TextJPN": "「あッ！@k@vS07/06/330600031.　写真ですね！@k@vS07/06/330600032.　写真撮影の専門誌のようです！！」",
		"NamesENG": "Shion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73462,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100111.",
		"TextJPN": "「他にもカメラのカタログまであるぜ！@k@vS07/01/330100112.　この雛見沢でこのキーワードが意味する人物はひとりしかいねぇだろ！！」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73463,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300041.",
		"TextJPN": "「うん、間違いないね！@k@vS07/03/330300042_1.　週刊激写別冊、ナースローアングル特集号まであるよ！！@k@vS07/03/330300043.　もう該当人物はひとりしかいないッ！！」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73464,
		"TextJPN": "…最後の一項目が大いに問題ありだが、とにかくとにかく、ここまで条件が揃えばあの人しかいないというわけだ。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73465,
		"TextJPN": "ったく、ぼんやりしているクセにレナの（強制的な）片想い相手ってのは納得行かねえぇ！",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73466,
		"NamesJPN": "レナ",
		"PreTextTags": "@vS07/02/330200092.",
		"TextJPN": "「はぅ～、レナにもカメラの使い方教えてくださいって言ったら教えてくれるかな、かな…！」",
		"NamesENG": "Rena",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73467,
		"NamesJPN": "レナ",
		"PreTextTags": "@vS07/02/330200093.",
		"TextJPN": "「そしたら手取り足取り教えてくれるかな、かなかなかな！@k@vS07/02/330200094.　ナースさんの格好して行ったらやさしくしてくれるかなッ！@k@vS07/02/330200095.　かなかなかなかなッ！！@k@vS07/02/330200096.　そしたらそしたら、はぅはぅ、はぅ～～～！！」",
		"NamesENG": "Rena",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73468,
		"TextJPN": "おもむろにレナが席を立つ！",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73469,
		"TextJPN": "瞳の中にはハートと花びらがいっぱいで、覗き込めばさぞやすごい光景が映っているに違いなかった。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73470,
		"NamesJPN": "レナ",
		"PreTextTags": "@vS07/02/330200097.",
		"TextJPN": "「あのねあのね魅ぃちゃん！@k@vS07/02/330200098.　レナは急に具合が悪くなっちゃったから早退するね、はぅ～！！」",
		"NamesENG": "Rena",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73471,
		"TextJPN": "何てメチャクチャ！　放課後を待たずにレナは教室を飛び出す！！",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73472,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300044.",
		"TextJPN": "「あぁああぁ、レナぁ！！@k@vS07/03/330300045.　お、いいところに立っているのは詩音！！@k@vS07/03/330300046.　レナを行かせるな、引き止めろぉおぉ！！」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73473,
		"NamesJPN": "詩音",
		"PreTextTags": "@vS07/06/330600033.",
		"TextJPN": "「む、無理ですよぉおおぉ！！@k@vS07/06/330600034.　さっきお姉が大変な目にあってたじゃないですかぁあぁ！！」",
		"NamesENG": "Shion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73474,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500107.",
		"TextJPN": "「……確かに、今のレナが相手じゃ部活メンバー総掛かりでも無理そうなのです」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73475,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300047.",
		"TextJPN": "「だからってまずい！！@k@vS07/03/330300048.　沙都子ッ！！」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73476,
		"TextJPN": "魅音が指をバチンと鳴らすと、沙都子の手元に天井から一本の紐が垂れてくる。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73477,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400127.",
		"TextJPN": "「あいあいさーですわぁ！！@k@vS07/04/330400128.　そおれッ！！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73478,
		"TextJPN": "それを勢いよく引いた時！　昇降口へ駆けて行くレナの足元にビーンと縄跳びが張られる仕掛けが作動する！",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73479,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400129.",
		"TextJPN": "「とりあえず転ばせて、その上からみんなで圧殺するんですのよ！！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73480,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400130.",
		"TextJPN": "「ふぎゃッ？！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73481,
		"TextJPN": "が、沙都子が言い終わる前に縄跳びが千切れたすごい音がした。……ナイロン製の縄跳びを引き千切るって、どういう怪力なんだろう…。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73482,
		"TextJPN": "縄跳び如きではレナを静止できず、もはや下駄箱で靴を履き替えている！",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73483,
		"NamesJPN": "知恵",
		"PreTextTags": "@vS07/22/VT2A_chie1001.",
		"TextJPN": "「こぉら！！@k@vS07/22/VT2A_chie1002.　廊下を走っちゃいけません！！」",
		"NamesENG": "Chie",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73484,
		"NamesJPN": "レナ",
		"PreTextTags": "@vS07/02/330200099.",
		"TextJPN": "「先生ごめんなさい、今日は早退します～～！！@k@vS07/02/330200100.　披露宴には呼びますからね！@k@vS07/02/330200101.　はぅ～！！」",
		"NamesENG": "Rena",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73485,
		"NamesJPN": "知恵",
		"PreTextTags": "@vS07/22/VT2A_chie1003.",
		"TextJPN": "「は、はぁ？！@k@vS07/22/VT2A_chie1004.　……ってお待ちなさい！@k@vS07/22/VT2A_chie1005.　ううぅ、教え子に先を越されたくなかった…！」",
		"NamesENG": "Chie",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73486,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300049.",
		"TextJPN": "「って、そこ落ち込むトコ違いますから！！」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73487,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300050.",
		"TextJPN": "「わ、私たちも早退します！@k@vS07/03/330300051.　とにかくレナより先に富竹さんを見つけて白い勾玉を奪おう！」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73488,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100113.",
		"TextJPN": "「それしかねぇな！@k@vS07/01/330100114.　富竹さんのいそうなところを捜そう！！」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73489,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400131.",
		"TextJPN": "「…梨花ぁ！@k@vS07/04/330400132.　あなたのインチキ妖気探知機で富竹さんを探し出してくださいませ！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73490,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500108.",
		"TextJPN": "「…みぃ、その語呂がとっても嫌な感じですが、…この方向は、古手神社なのです！@k@vS07/05/330500109.　富竹は昨日撮影に来ると言っていましたから、たぶん境内だと思いますです！@k@vS07/05/330500110.　きっと鷹野も一緒なのです！」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73491,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400133.",
		"TextJPN": "「…あぁ素敵ですわ。@k@vS07/04/330400134.ほんと、お約束万歳でございますわ！@k@vS07/04/330400135.　こーゆう時のカップルって必ず二人一緒なんでしょうけど」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73492,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400136.",
		"TextJPN": "「魅音さん、富竹さんは神社だって梨花が申しておりますわ！！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73493,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300052.",
		"TextJPN": "「梨花ちゃん、そうなの？！@k@vS07/03/330300053.　私もそう思ってたとこだわ！！」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73494,
		"NamesJPN": "詩音",
		"PreTextTags": "@vS07/06/330600035.",
		"TextJPN": "「でもそれはレナさんも思っているみたいですよ！！@k@vS07/06/330600036.　神社の方向へ走って…って、うわ速っ？！」",
		"NamesENG": "Shion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73495,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100115.",
		"TextJPN": "「何て速度だ！！@k@vS07/01/330100116.　見る見る視界から遠のいていくぞ…！！」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73496,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300054.",
		"TextJPN": "「それでも追うしかないよ！@k@vS07/03/330300055.　レナが富竹さんを口説き出す前に何とかしなきゃ！」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73497,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500111.",
		"TextJPN": "「……で、鷹野が妬いて気まずい三角関係で大変なのです。@k@vS07/05/330500112.にぱ～☆」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73498,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400137.",
		"TextJPN": "「まぁ、別にあのカップルが破局するのは構いませんけど…」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73499,
		"NamesJPN": "詩音",
		"PreTextTags": "@vS07/06/330600037.",
		"TextJPN": "「…八つ当たりで健康診断のときに、注射の本数増やされるかもですよ？」",
		"NamesENG": "Shion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73500,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400138.",
		"TextJPN": "「じょ、冗談じゃありませんわ～ッッ！！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 73501,
		"TextJPN": "そうこうするうちに、レナはとっくに俺たちの視界から消え去っている。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
