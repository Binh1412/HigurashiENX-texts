[
	{
		"type": "SAVEINFO",
		"category": 1,
		"JPN": "BADEND (敗退)",
		"ENG": "BADEND (敗退)"
	},
	{
		"type": "MSGSET",
		"MessageID": 74335,
		"TextJPN": "カキィィィ――ンンッッ！！！",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74336,
		"NamesJPN": "亀田",
		"PreTextTags": "@vS07/23/VT2A_kameda1021.",
		"TextJPN": "「ば、ばかなぁ…っ？！」",
		"NamesENG": "Kameda",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74337,
		"TextJPN": "今度はマウンド上で、がっくりと膝をつき崩れ落ちる亀田くん。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74338,
		"TextJPN": "レナのこの日３本目のアーチが、センター方向のゾーンへと消えていった。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74339,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400254.",
		"TextJPN": "「け、圭一さん…？」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74340,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100337.",
		"TextJPN": "「心配するな！@k@vS07/01/330100338.　俺の真の力をとくと見せてやるぜぇぇっ！！」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74341,
		"TextJPN": "そして俺は、打席に入るとグリップを握り締め、気迫を込めてバットを構えた。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74342,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100339.",
		"TextJPN": "「さぁ、どっからでもきやがれぇぇッッ！！！」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74343,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400255.",
		"TextJPN": "「だから申し上げましたのに～～！！@k@vS07/04/330400256.　あれだけ大口叩いて７球連続空振りって、どういうことなんですの～～！！！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74344,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100340.",
		"TextJPN": "「す、すまっ、…がはっ？！」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74345,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100341.",
		"TextJPN": "「い、いやこんなはずじゃ、ぼげっ？！」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74346,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100342.",
		"TextJPN": "「だ、だから俺の話を、ふぐっ？！」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74347,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500179.",
		"TextJPN": "「…お仕置きなのです。@k@vS07/05/330500180.ボクたちの期待を裏切った愚か者には、思いっきりおしおきなのですよ～！！」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74348,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100343.",
		"TextJPN": "「ごへっ！　ごはっ！　ひげっ！　へごっ！！」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74349,
		"TextJPN": "本気のレナに無策で立ち向かった俺に待っていたのは、まさに鎧袖一触の惨敗。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74350,
		"TextJPN": "そして、部活メンバーからのお仕置きという名の『タコ殴り』だった。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74351,
		"NamesJPN": "詩音",
		"PreTextTags": "@vS07/06/330600107.",
		"TextJPN": "「ほら、…サヨナラホームランを打った打者を出迎えるときに他の選手がやる、手荒い祝福ってやつ？」",
		"NamesENG": "Shion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74352,
		"NamesJPN": "詩音",
		"PreTextTags": "@vS07/06/330600108.",
		"TextJPN": "「…あれの正反対の意味合いで、『憎しみ』『怒り』が入ったと考えてくださいな」",
		"NamesENG": "Shion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74353,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300177.",
		"TextJPN": "「それにしては、なんか変な凶器混ざってない？」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74354,
		"TextJPN": "…とりあえず、その後沙都子と梨花の『必殺泣き落とし戦法』で監督を説得し、レナの勾玉の呪いは解決することができた。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74355,
		"TextJPN": "ただ、その交換条件として『沙都子と梨花ちゃんがメイド姿でご奉仕』を泣く泣く飲むしかなく、その実害が直撃した二人は特に恨みつらみで俺に報復を、",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74356,
		"TextJPN": "――痛っ！　痛っ、痛っ、痛ぁッッ？！",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74357,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300178.",
		"TextJPN": "「まぁ、ひとまずは一件落着ってことで…いいよね？」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74358,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100344.",
		"TextJPN": "「お、俺は全然良くねぇぞ！@k@vS07/01/330100345.　だ、誰か助けてくれぇ～～！！@k@vS07/01/330100346.　レナぁぁ～～！！」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74359,
		"NamesJPN": "レナ",
		"PreTextTags": "@vS07/02/330200312.",
		"TextJPN": "「…はう？@k@vS07/02/330200313.　圭一くん、何か悪いことしたのかな？　かな？」",
		"NamesENG": "Rena",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74360,
		"TextJPN": "…………。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74361,
		"TextJPN": "レナの、渾身の力を込めたストレート。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74362,
		"TextJPN": "それを、俺は目一杯の力でフルスイング。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74363,
		"TextJPN": "…そしてボールは、外野の奥深くの場所へと、運ばれていった…。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74364,
		"NamesJPN": "入江",
		"PreTextTags": "@vS07/10/331000183.",
		"TextJPN": "「…というわけで。@k@vS07/10/331000184.ホームラン競争は、３対３、ドロー！@k@vS07/10/331000185.　しかし竜宮さんがギブアップのため、部活チームの勝ちです！！」",
		"NamesENG": "Irie",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74365,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100347.",
		"TextJPN": "「ぃよっしゃぁあぁぁあぁっっ！！」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74366,
		"TextJPN": "…勝利に沸き立つ俺たちとは対照的に、がっくりとうなだれるレナ。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74367,
		"TextJPN": "俺に投げた最後の一球。…文字通りあれが、レナにとって渾身の一球だったのだ。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74368,
		"TextJPN": "レナは投げ終わったあと、全力を出し切ったようにマウンド上へ膝をつき…。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74369,
		"TextJPN": "それでも、まだ戦おうと懸命に立ち上がりかけたが、見かねた監督が最後の『ご主人様の命令』として渋るレナにギブアップを命じたことで、ついに決着となった。",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74370,
		"NamesJPN": "レナ",
		"PreTextTags": "@vS07/02/330200314.",
		"TextJPN": "「あああぁあぁぁ…、はぅーー、負けちゃったぁあぁ……」",
		"NamesENG": "Rena",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74371,
		"NamesJPN": "入江",
		"PreTextTags": "@vS07/10/331000186.",
		"TextJPN": "「いえいえ！@k@vS07/10/331000187.　勝負には負けましたが、レナさんは孤軍奮闘、素晴らしい戦いっぷりでした！@k@vS07/10/331000188.　同じ条件で戦っていれば、私たちの大勝利でしたよ！」",
		"NamesENG": "Irie",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74372,
		"NamesJPN": "入江",
		"PreTextTags": "@vS07/10/331000189.",
		"TextJPN": "「いや、今回のこれは勝ち負けじゃない。@k@vS07/10/331000190.本当に素晴らしい、私にとっては最高に実りある体験でした！」",
		"NamesENG": "Irie",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74373,
		"NamesJPN": "入江",
		"PreTextTags": "@vS07/10/331000191.",
		"TextJPN": "「せっかくのメイドを失うのは惜しいですが、これでまたメイド化計画を実現する野望が一層強くなりましたよ！！@k@vS07/10/331000192.　竜宮さん、ステキなドリームをありがとうございました！！」",
		"NamesENG": "Irie",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74374,
		"NamesJPN": "レナ",
		"PreTextTags": "@vS07/02/330200315.",
		"TextJPN": "「……はぅ…。@k@vS07/02/330200316.…はぁい…」",
		"NamesENG": "Rena",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74375,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400257.",
		"TextJPN": "「勝負には勝ちましたが、他のもので負けた気がしますわね」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74376,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100348.",
		"TextJPN": "「…へへへ、さすがレナってとこかぁ。@k@vS07/01/330100349.…はぁ～！@k@vS07/01/330100350.　緊張感が抜けたら、一気に疲れが…」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74377,
		"NamesJPN": "入江",
		"PreTextTags": "@vS07/10/331000193.",
		"TextJPN": "「そうそう、このキーホルダーをお返しする約束でしたねぇ、…おっとぉっ？！」",
		"NamesENG": "Irie",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74378,
		"NamesJPN": "入江",
		"PreTextTags": "@vS07/10/331000194.",
		"TextJPN": "「あ、すみませんッ！！」",
		"NamesENG": "Irie",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74379,
		"TextJPN": "監督がユニフォームの胸ポケットから勾玉を摘み出した時、ツルンと指先で滑ってそれを弾き飛ばしてしまう！　って、……なな、なんでタイミングよく俺が大口開けたところに向かって飛んで来るんだ？！",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74380,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100351.",
		"TextJPN": "「んがッ？！」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74381,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100352.",
		"TextJPN": "「んがんっぐッ！！！」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74382,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100353.",
		"TextJPN": "「ああぁああぁ？！」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74383,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100354.",
		"TextJPN": "「のの、飲み込んじまったぁああぁ！！」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74384,
		"NamesJPN": "魅音",
		"PreTextTags": "@vS07/03/330300179.",
		"TextJPN": "「は、はぁ？！@k@vS07/03/330300180.　何やってんの圭ちゃん！！@k@vS07/03/330300181.　吐き出して吐き出して！！@k@vS07/03/330300182.　おじさんが下腹を殴ってあげるよ、沙都子、羽交い絞めにして！！@k@vS07/03/330300182_1.　それ、ドスドス！！」",
		"NamesENG": "Mion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74385,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100355.",
		"TextJPN": "「ぐぇ、ぐわ！！@k@vS07/01/330100356.　や、やめてぇえぇ！！」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74386,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400258.",
		"TextJPN": "「神妙になさいませっぇえええぇ！！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74387,
		"NamesJPN": "圭一",
		"PreTextTags": "@vS07/01/330100357.",
		"TextJPN": "「ふぐお、ぐぇお！！@k@vS07/01/330100358.　ボディは後から効いてくるぅうぅ！！」",
		"NamesENG": "Keiichi",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74388,
		"NamesJPN": "詩音",
		"PreTextTags": "@vS07/06/330600109.",
		"TextJPN": "「り、……梨花ちゃま…。@k@vS07/06/330600110.これって手遅れ？」",
		"NamesENG": "Shion",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74389,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500181.",
		"TextJPN": "「…みぃっ！！@k@vS07/05/330500182.　お腹の中に入ったら、さらっと溶けてあっさり吸収してしまいます！@k@vS07/05/330500183.　食後３０分以内がタイムリミットなのです！！」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74390,
		"NamesJPN": "梨花",
		"PreTextTags": "@vS07/05/330500184.",
		"TextJPN": "「でも、とにかく紅白揃いましたのです！@k@vS07/05/330500185.　圭一とレナを古手神社に連れて行けば、すぐにボクが解呪させますのです！」",
		"NamesENG": "Rika",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74391,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400259.",
		"TextJPN": "「というわけで、二人が取り返しのつかないことになる前に古手神社へ連行ですわ！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74392,
		"NamesJPN": "沙都子",
		"PreTextTags": "@vS07/04/330400260.",
		"TextJPN": "「さぁさぁ魅音さんに詩音さん！@k@vS07/04/330400261.　二人を引き摺って行きますわよぉー！！」",
		"NamesENG": "Satoko",
		"TextENG": ""
	},
	{
		"type": "MSGSET",
		"MessageID": 74393,
		"NamesJPN": "魅音＆詩音",
		"PreTextTags": "@o75.@vS07/03/330300183|S07/06/330600111.",
		"TextJPN": "「「あいあいさーー！！！」」",
		"NamesENG": "Mion & Shion",
		"TextENG": ""
	}
]